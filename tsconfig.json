{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
        "resolveJsonModule": true,
"esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
